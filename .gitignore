/vendor
composer.lock

# Apache Ant
/.ant_targets

# Build artifacts and temporary files
/build
/build/artifacts
/build/tmp
/tests/autoload.php

# PHP-CS-Fixer
/.php-cs-fixer.cache

# Psalm
/.psalm/cache

# PHPUnit
.phpunit.result.cache
/.phpunit.cache

